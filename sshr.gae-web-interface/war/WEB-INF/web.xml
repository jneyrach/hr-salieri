<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	     xmlns="http://java.sun.com/xml/ns/javaee" 
		 xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	     xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	     version="2.5">
		 
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>AsistenciaCrudCreatePerfilHorario</servlet-name>
		<servlet-class>sshr.web.servlet.asistencia.CrudCreatePerfilHorario</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AsistenciaCrudUpdatePerfilHorario</servlet-name>
		<servlet-class>sshr.web.servlet.asistencia.CrudUpdatePerfilHorario</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AsistenciaCrudRemovePerfilHorario</servlet-name>
		<servlet-class>sshr.web.servlet.asistencia.CrudRemovePerfilHorario</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AsistenciaCrudFindPerfilHorario</servlet-name>
		<servlet-class>sshr.web.servlet.asistencia.CrudFindPerfilHorario</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AsistenciaCrudFindAllPerfilHorario</servlet-name>
		<servlet-class>sshr.web.servlet.asistencia.CrudFindAllPerfilHorario</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>AsistenciaCrudCreatePerfilHorario</servlet-name>
		<url-pattern>/AsistenciaCrudCreatePerfilHorario</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsistenciaCrudUpdatePerfilHorario</servlet-name>
		<url-pattern>/AsistenciaCrudUpdatePerfilHorario</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsistenciaCrudRemovePerfilHorario</servlet-name>
		<url-pattern>/AsistenciaCrudRemovePerfilHorario</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsistenciaCrudFindPerfilHorario</servlet-name>
		<url-pattern>/AsistenciaCrudFindPerfilHorario</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsistenciaCrudFindAllPerfilHorario</servlet-name>
		<url-pattern>/AsistenciaCrudFindAllPerfilHorario</url-pattern>
	</servlet-mapping>
	
</web-app>