<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	     xmlns="http://java.sun.com/xml/ns/javaee" 
		 xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	     xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	     version="2.5">
		 
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>AsistenciaCrudCreateGrupoHorario</servlet-name>
		<servlet-class>sshr.web.servlet.asistencia.CrudCreateGrupoHorario</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AsistenciaCrudUpdateGrupoHorario</servlet-name>
		<servlet-class>sshr.web.servlet.asistencia.CrudUpdateGrupoHorario</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AsistenciaCrudRemoveGrupoHorario</servlet-name>
		<servlet-class>sshr.web.servlet.asistencia.CrudRemoveGrupoHorario</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AsistenciaCrudFindGrupoHorario</servlet-name>
		<servlet-class>sshr.web.servlet.asistencia.CrudFindGrupoHorario</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AsistenciaCrudFindAllGrupoHorario</servlet-name>
		<servlet-class>sshr.web.servlet.asistencia.CrudFindAllGrupoHorario</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>AsistenciaCrudCreateGrupoHorario</servlet-name>
		<url-pattern>/AsistenciaCrudCreateGrupoHorario</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsistenciaCrudUpdateGrupoHorario</servlet-name>
		<url-pattern>/AsistenciaCrudUpdateGrupoHorario</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsistenciaCrudRemoveGrupoHorario</servlet-name>
		<url-pattern>/AsistenciaCrudRemoveGrupoHorario</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsistenciaCrudFindGrupoHorario</servlet-name>
		<url-pattern>/AsistenciaCrudFindGrupoHorario</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsistenciaCrudFindAllGrupoHorario</servlet-name>
		<url-pattern>/AsistenciaCrudFindAllGrupoHorario</url-pattern>
	</servlet-mapping>
	
</web-app>